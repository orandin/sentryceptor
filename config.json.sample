{
    "host": "localhost",
    "port": 8080,

    "router": {
        "/YOUR_ENDPOINT": {
            "url": "http://sentry.io/TO_UPDATE",
            "filter": {
                "tags": [
                    {
                        "conditions": [
                            {
                                "comparator": "contains",
                                "path": "instance",
                                "value": "awesome.io"
                            }
                        ]
                    }
                ],
                "breadcrumbs": [
                    {
                        "conditions": [
                            {
                                "comparator": "contains",
                                "path": "level",
                                "value": "debug"
                            }
                        ]
                    }
                ]
            }
        }
    }
}
